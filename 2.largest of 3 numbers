public class Main {
    public static void main(String[] args) {
      int a=20;
      int b=19;
      int c=18;
      if(a>b && a>c){
        System.out.println("the big number is"+a);
      }else if(b>c){
        System.out.println("the big number is:"+b);
      }else{
      System.out.println("the big number is :"+c);
      }
  }
}
