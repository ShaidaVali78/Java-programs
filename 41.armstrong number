class Main {
    public static void main(String[] args) {
        int n=153;
        int sum=0;
        int b=n;
        while(n!=0){
            int rem=n%10;
            sum=(rem*rem*rem)+sum;
            n=n/10;
        }
           System.out.println(sum);

        if(sum==b) {
            System.out.println("given number is armstrong number");
        }else {
        System.out.println("given number is not armstrong number");
        }
    }
}
