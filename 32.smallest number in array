class Main {
    public static void main(String[] args) {
        int[] arr={12,91,82,76,97};
        int small=arr[0];
        for(int i=1; i<arr.length; i++) {
            if(arr[i]<=small){
                small=arr[i];
            }
        }
        System.out.println("smallest number in array is"+ small);
    }
}
