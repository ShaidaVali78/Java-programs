public class Main {
    public static void main(String[] args) {
      int a=10;
      int b=12;
      int c=1;
      if(a==b && b==c && c==a){
        System.out.println("eqilatral triangle");
      }else if(a==b ||b==c || c==a) {
        System.out.println("isosceles triangle");
      }else{
        System.out.println("scalene triangle");
      }
  }
}
