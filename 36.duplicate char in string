class Main {
    public static void main(String[] args) {
        String s="aAeEcdDCcffF";
        StringBuilder st=new StringBuilder();
        for(int i=0; i<s.length(); i++) {
           if(st.indexOf(String.valueOf(s.charAt(i)))==-1) {
            st.append(s.charAt(i));
           }
        
        }
        System.out.println(st);
    }
}
