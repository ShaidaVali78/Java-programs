class Main {
    public static void main(String[] args) {
        int[] arr={12,91,82,76,97};
        int large=arr[0];
        for(int i=1; i<arr.length; i++) {
            if(arr[i]>=large){
                large=arr[i];
            }
        }
        System.out.println("largest number in array is"+ large);
    }
}
