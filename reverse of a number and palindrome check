class Main {
    public static void main(String[] args) {
        int n=786;
        int b=n;
        int rev=0;
        while(n!=0){
            int rem=n%10;
            rev=rev*10+rem;
            n=n/10;
        }
        System.out.println(rev);
        if(rev==b) {
        System.out.println("palindrome");
       }else {
        System.out.println("not palindrome");
       }
    }
}
